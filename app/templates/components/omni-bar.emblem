play-bar playbar=true inline=true buttons=left
component [
  (concat song.barType "-bar")
  class='big-me'
  beatMask=beatMask
  ]


play-bar playbar=true inline=true buttons=right

.beat-blanket
  each song.measureKit as |kitBin index|
    =beat-bar  [
      index=index
      boundValue=song.beat
      kitBin=(mut kitBin)
    ]


